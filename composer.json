{
  "name": "sankar/to-twig",
  "type": "application",
  "description": "A script to convert smarty template engine to twig",
  "license": "MIT",
  "authors": [
    {
      "name": "sankar",
      "email": "sankar.suda@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "symfony/console": "^2.1",
    "symfony/filesystem": "^2.1",
    "symfony/finder": "^2.1",
    "sebastian/diff": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^3.7.0"
  },
  "autoload": {
    "psr-4": {
      "toTwig\\": "app/toTwig/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "bin": [
    "toTwig"
  ]
}
